<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= homeContent %></p>

  <% posts.forEach(function(post) { %> <!-- using javascript forEach method on an array instead of for loop -->
    <h1><%= post.titleBody %></h1>
    <p><%= post.postBody.substring(0, 100).concat("...") %>
    <a href=/posts/<%= post.titleBody %>>Read More</a>
    </p>
  <% }) %>


<%- include('partials/footer') -%>
